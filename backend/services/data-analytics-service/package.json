{
  "name": "data-analytics-service",
  "version": "1.0.0",
  "description": "A high-performance service for logging and analyzing user actions in projects, built with TypeScript and Node.js.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --write '**/*.{js,ts,json,md}'",
    "dev": "nodemon dist/index.js",
    "prepare": "husky install",
    "precommit": "npm run lint && npm run test",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Elmahrosa/data-analytics-service.git"
  },
  "keywords": [
    "analytics",
    "data",
    "service",
    "logging",
    "typescript",
    "nodejs",
    "performance"
  ],
  "author": "KOSASIH",
  "license": "MIT",
  "dependencies": {
    "express": "^4.17.1",
    "mongoose": "^6.0.0",
    "dotenv": "^10.0.0",
    "cors": "^2.8.5",
    "helmet": "^4.6.0",
    "compression": "^1.7.4"
  },
  "devDependencies": {
    "typescript": "^4.5.0",
    "ts-jest": "^27.0.0",
    "jest": "^27.0.0",
    "eslint": "^8.0.0",
    "prettier": "^2.5.0",
    "husky": "^7.0.0",
    "nodemon": "^2.0.15",
    "@types/jest": "^27.0.0",
    "@types/node": "^16.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/Elmahrosa/data-analytics-service#readme",
  "bugs": {
    "url": "https://github.com/Elmahrosa/data-analytics-service/issues"
  },
  "funding": {
    "url": "https://github.com/sponsors/Elmahrosa"
  }
